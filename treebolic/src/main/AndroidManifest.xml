<?xml version="1.0" encoding="utf-8"?><!--  ~ Copyright (c) Treebolic 2019. Bernard Bou <1313ou@gmail.com>  --><manifest xmlns:android="http://schemas.android.com/apk/res/android"	xmlns:tools="http://schemas.android.com/tools"	android:installLocation="auto"	android:sharedUserId="org.treebolic.userId"	android:sharedUserMaxSdkVersion="32"	tools:targetApi="tiramisu">	<supports-screens		android:largeScreens="true"		android:normalScreens="true"		android:requiresSmallestWidthDp="360"		android:smallScreens="false"		android:xlargeScreens="true" />	<uses-permission		android:name="android.permission.READ_EXTERNAL_STORAGE"		android:maxSdkVersion="32" />	<uses-permission		android:name="android.permission.WRITE_EXTERNAL_STORAGE"		android:maxSdkVersion="18" />	<uses-permission android:name="android.permission.INTERNET" />	<uses-permission android:name="org.treebolic.service.USE" />	<queries>		<intent>			<action android:name="org.treebolic.service.action.MAKE_MODEL" />		</intent>	</queries>	<application		android:allowBackup="true"		android:dataExtractionRules="@xml/data_extraction_rules"		android:enableOnBackInvokedCallback="true"		android:fullBackupContent="@xml/backup_descriptor"		android:icon="@mipmap/ic_launcher"		android:label="@string/app_name"		android:process="org.treebolic"		android:roundIcon="@mipmap/ic_launcher_round"		android:supportsRtl="true"		android:theme="@style/MyTheme">		<activity			android:name="org.treebolic.MainActivity"			android:exported="true"			android:label="@string/title_activity_main"			android:launchMode="singleTop">			<intent-filter>				<action android:name="android.intent.action.MAIN" />				<category android:name="android.intent.category.LAUNCHER" />			</intent-filter>		</activity>		<activity			android:name="org.treebolic.TreebolicActivity"			android:exported="true"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.TreebolicClientActivity"			android:exported="true"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_client"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.TreebolicModelActivity"			android:exported="true"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_model"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.SettingsActivity"			android:exported="false"			android:label="@string/title_activity_settings"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.ServicesActivity"			android:exported="false"			android:label="@string/title_activity_services"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.filechooser.FileChooserActivity"			android:exported="false"			android:parentActivityName="org.treebolic.MainActivity"			tools:ignore="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.ProvidersActivity"			android:exported="false"			android:label="@string/title_activity_providers"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.DownloadActivity"			android:exported="false"			android:label="@string/title_activity_download"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.guide.HelpActivity"			android:exported="false"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="org.treebolic.guide.AboutActivity"			android:exported="false"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="com.bbou.others.OthersActivity"			android:exported="false"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>		<activity			android:name="com.bbou.donate.DonateActivity"			android:exported="false"			android:parentActivityName="org.treebolic.MainActivity">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value="org.treebolic.MainActivity" />		</activity>	</application></manifest>