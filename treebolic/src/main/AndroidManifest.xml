<?xml version="1.0" encoding="utf-8"?><!--  ~ Copyright (c) Treebolic 2019. Bernard Bou <1313ou@gmail.com>  --><manifest xmlns:android="http://schemas.android.com/apk/res/android"	xmlns:tools="http://schemas.android.com/tools"	package="org.treebolic"	android:installLocation="auto"	android:sharedUserId="org.treebolic.userId">	<supports-screens		android:largeScreens="true"		android:normalScreens="true"		android:requiresSmallestWidthDp="360"		android:smallScreens="false"		android:xlargeScreens="true" />	<uses-permission		android:name="android.permission.WRITE_EXTERNAL_STORAGE"		tools:ignore="ScopedStorage" />	<uses-permission android:name="android.permission.INTERNET" />	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />	<queries>		<package android:name="org.treebolic.files.service" />		<package android:name="org.treebolic.owl.service" />		<package android:name="org.treebolic.wordnet.service" />	</queries>	<application		android:allowBackup="true"		android:fullBackupContent="@xml/backup_descriptor"		android:icon="@mipmap/ic_launcher"		android:label="@string/app_name"		android:process="org.treebolic"		android:roundIcon="@mipmap/ic_launcher_round"		android:supportsRtl="true"		android:theme="@style/MyTheme"		tools:ignore="UnusedAttribute">		<activity			android:name=".MainActivity"			android:label="@string/title_activity_main"			android:launchMode="singleTop">			<intent-filter>				<action android:name="android.intent.action.MAIN" />				<category android:name="android.intent.category.LAUNCHER" />			</intent-filter>		</activity>		<activity			android:name=".TreebolicActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".TreebolicClientActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_client"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".TreebolicModelActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_model"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".TreebolicPluginActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_plugin"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".SettingsActivity"			android:label="@string/title_activity_settings"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".ServicesActivity"			android:label="@string/title_activity_services"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".filechooser.FileChooserActivity"			android:parentActivityName=".MainActivity"			tools:ignore="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".ProvidersActivity"			android:label="@string/title_activity_providers"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".DownloadActivity"			android:label="@string/title_activity_download"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".guide.HelpActivity"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".guide.AboutActivity"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name="com.bbou.others.OthersActivity"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name="com.bbou.donate.DonateActivity"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>	</application></manifest>