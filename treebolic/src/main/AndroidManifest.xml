<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"          xmlns:tools="http://schemas.android.com/tools"          package="org.treebolic"          android:installLocation="auto"          android:sharedUserId="org.treebolic.userId"          android:versionCode="20"          android:versionName="1.20">	<supports-screens		android:largeScreens="true"		android:normalScreens="true"		android:requiresSmallestWidthDp="360"		android:smallScreens="false"		android:xlargeScreens="true"/>	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>	<uses-permission android:name="android.permission.INTERNET"/>	<application		android:allowBackup="true"		android:icon="@mipmap/ic_launcher"		android:label="@string/app_name"		android:process="org.treebolic"		android:supportsRtl="true"		android:theme="@style/MyTheme"		tools:ignore="AllowBackup,UnusedAttribute">		<activity			android:name=".MainActivity"			android:label="@string/title_activity_main"			android:launchMode="singleTop">			<intent-filter>				<action android:name="android.intent.action.MAIN"/>				<category android:name="android.intent.category.LAUNCHER"/>			</intent-filter>		</activity>		<activity			android:name=".TreebolicActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".TreebolicClientActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_client"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".TreebolicModelActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_model"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".TreebolicPluginActivity"			android:hardwareAccelerated="true"			android:label="@string/title_activity_treebolic_plugin"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".SettingsActivity"			android:label="@string/title_activity_settings"			android:parentActivityName="org.treebolic.MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".ServicesActivity"			android:label="@string/title_activity_services"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".ProvidersActivity"			android:label="@string/title_activity_providers"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name=".DownloadActivity"			android:label="@string/title_activity_download"			android:parentActivityName=".MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name="org.treebolic.guide.HelpActivity"			android:parentActivityName="org.treebolic.MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name="org.treebolic.guide.AboutActivity"			android:parentActivityName="org.treebolic.MainActivity"			tools:targetApi="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>		<activity			android:name="org.treebolic.filechooser.FileChooserActivity"			android:parentActivityName="org.treebolic.MainActivity"			tools:ignore="jelly_bean">			<meta-data				android:name="android.support.PARENT_ACTIVITY"				android:value=".MainActivity" />		</activity>	</application></manifest>